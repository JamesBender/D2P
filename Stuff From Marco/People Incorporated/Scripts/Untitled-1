IF OBJECT_ID('U_ESTDLIFNEI_File') IS NULL
CREATE TABLE [dbo].[U_ESTDLIFNEI_File] (
    [RecordSet] char(3) NOT NULL,
    [InitialSort] varchar(100) NOT NULL,
    [SubSort] varchar(100) NOT NULL,
    [SubSort2] varchar(100) NULL,
    [SubSort3] varchar(100) NULL,
    [Data] varchar(max) NULL
);
IF OBJECT_ID('U_ESTDLIFNEI_Spouse') IS NULL
CREATE TABLE [dbo].[U_ESTDLIFNEI_Spouse] (
    [SpsEEID] char(12) NOT NULL,
    [SpsNameFirst] varchar(100) NULL,
    [SpsNameLast] varchar(100) NULL,
    [SpsDateOfBirth] datetime NULL,
    [SpsRelationship] varchar(7) NULL,
    [SpsGender] char(1) NULL
);
IF OBJECT_ID('U_ESTDLIFNEI_Trailer') IS NULL
CREATE TABLE [dbo].[U_ESTDLIFNEI_Trailer] (
    [drvRecordCount] int NULL
);
GO
CREATE PROCEDURE [dbo].[dsi_sp_BuildDriverTables_ESTDLIFNEI]
    @SystemID char(12)
AS
SET NOCOUNT ON;
/**********************************************************************************
Client Name: Neighborcare Health

Created By: Joseph Pham
Business Analyst: Courtney Chaffer
Create Date: 03/14/2018
Service Request Number: SR-2017-00176488

Purpose: The Standard Life Export

Revision History
----------------
Update By           Date           Request Num        Desc
XXXX                XX/XX/2018     SR-2018-000XXXXX   XXXXX

SELECT * FROM dbo.U_dsi_Configuration WHERE FormatCode = 'ESTDLIFNEI';
SELECT * FROM dbo.U_dsi_SqlClauses WHERE FormatCode = 'ESTDLIFNEI';
SELECT * FROM dbo.U_dsi_Parameters WHERE FormatCode = 'ESTDLIFNEI';
SELECT ExpFormatCode, ExpExportCode, ExpStartPerControl, ExpEndPerControl,* FROM dbo.AscExp WHERE expFormatCode = 'ESTDLIFNEI';
SELECT * FROM dbo.U_dsi_InterfaceActivityLog WHERE FormatCode = 'ESTDLIFNEI' ORDER BY RunID DESC;

Execute Export
--------------
EXEC dbo.dsi_sp_TestSwitchbox_v2 'ESTDLIFNEI', 'ONDEMAND';
EXEC dbo.dsi_sp_TestSwitchbox_v2 'ESTDLIFNEI', 'OEPASSIVE';
EXEC dbo.dsi_sp_TestSwitchbox_v2 'ESTDLIFNEI', 'OEACTIVE';
EXEC dbo.dsi_sp_TestSwitchbox_v2 'ESTDLIFNEI', 'TEST';

EXEC dbo.dsi_BDM_sp_ErrorCheck 'ESTDLIFNEI';

EXEC dbo._dsi_usp_ExportRipOut @FormatCode = 'ESTDLIFNEI', @AllObjects = 'Y', @IsWeb = 'Y'
**********************************************************************************/
BEGIN

    --==========================================
    -- Declare variables
    --==========================================
    DECLARE  @FormatCode        VARCHAR(10)
            ,@ExportCode        VARCHAR(10)
            ,@StartDate         DATETIME
            ,@EndDate           DATETIME
            ,@StartPerControl   VARCHAR(9)
            ,@EndPerControl     VARCHAR(9);

    -- Set FormatCode
    SELECT @FormatCode = 'ESTDLIFNEI';

    -- Declare dates from Parameter file
    SELECT
         @StartPerControl = StartPerControl
        ,@EndPerControl   = EndPerControl
        ,@StartDate       = LEFT(StartPerControl,8)
        ,@EndDate         = DATEADD(S,-1,DATEADD(D,1,LEFT(EndPerControl,8)))
        ,@ExportCode      = ExportCode
    FROM dbo.U_dsi_Parameters WITH (NOLOCK)
    WHERE FormatCode = @FormatCode;

    --==========================================
    -- Clean EE List 
    -- Caution: Careful of cleaning EE List if including paycheck data
    --==========================================

    -- Cleans EE List of terms where EE active in another company (transfer), or active in more than one company
    DELETE FROM dbo.U_ESTDLIFNEI_EEList
    WHERE xCoID <> dbo.dsi_BDM_fn_GetCurrentCOID(xEEID)
    AND xEEID IN (SELECT xEEID FROM dbo.U_ESTDLIFNEI_EEList GROUP BY xEEID HAVING COUNT(1) > 1);

    --==========================================
    -- Audit Section
    --==========================================
    -- Get data from audit fields table. Add fields here if auditing
    IF OBJECT_ID('U_ESTDLIFNEI_AuditFields','U') IS NOT NULL
    DROP TABLE dbo.U_ESTDLIFNEI_AuditFields;
    CREATE TABLE dbo.U_ESTDLIFNEI_AuditFields (aTableName varchar(30),aFieldName varchar(30));

    INSERT INTO dbo.U_ESTDLIFNEI_AuditFields VALUES ('EmpComp','EecFullTimeOrPartTime')

    -- Create audit table based on fields defined above
    IF OBJECT_ID('U_ESTDLIFNEI_Audit','U') IS NOT NULL
        DROP TABLE dbo.U_ESTDLIFNEI_Audit;
    SELECT 
        audEEID = audKey1Value
        ,audKey2 = audKey2Value
        ,audKey3 = audKey3Value
        ,audTableName
        ,audFieldName
        ,audAction
        ,audDateTime
        ,audOldValue
        ,audNewValue
    INTO dbo.U_ESTDLIFNEI_Audit
    FROM dbo.U_ESTDLIFNEI_EEList WITH (NOLOCK)
    JOIN dbo.U_ESTDLIFNEI_AuditFields WITH (NOLOCK) 
        ON 1 = 1
    JOIN dbo.vw_AuditData WITH (NOLOCK) 
        ON audKey1Value = xEEID
        AND audTableName = aTableName
        AND audFieldName = aFieldName
    WHERE audAction <> 'DELETE'
    AND ISNULL(audNewValue,'') <> ''
    AND (audOldValue = 'P' AND audNewValue = 'F');

    --==========================================
    -- BDM Section
    --==========================================
    DELETE FROM dbo.U_dsi_BDM_Configuration WHERE FormatCode = @FormatCode;

    -- Required parameters
    INSERT INTO dbo.U_dsi_BDM_Configuration VALUES(@FormatCode,'DedCodes','GLIFE,LIFEE,VADD,LIFES,VADDS,LIFEC,VADDC,ADD,LTD,LIFEF');
    INSERT INTO dbo.U_dsi_BDM_Configuration VALUES(@FormatCode,'StartDateTime',@StartDate);
    INSERT INTO dbo.U_dsi_BDM_Configuration VALUES(@FormatCode,'EndDateTime',@EndDate);
    INSERT INTO dbo.U_dsi_BDM_Configuration VALUES(@FormatCode,'TermSelectionOption','AuditDate');

    -- Required OE parameters
    IF @ExportCode LIKE '%PASSIVE'
    BEGIN
        INSERT INTO dbo.U_dsi_BDM_Configuration VALUES (@FormatCode,'OEType','PASSIVE');
    END;

    IF @ExportCode LIKE '%ACTIVE'
    BEGIN
        INSERT INTO dbo.U_dsi_BDM_Configuration VALUES (@FormatCode,'OEType','ACTIVE');
    END;

    -- Run BDM Module
    EXEC dbo.dsi_BDM_sp_PopulateDeductionsTable @FormatCode;

    --Get spouses and domestic partners
    IF OBJECT_ID('U_ESTDLIFNEI_Spouse','U') IS NOT NULL
        DROP TABLE dbo.U_ESTDLIFNEI_Spouse;
    SELECT DISTINCT
         SpsEEID = dbnEEID
        ,SpsNameFirst = ConNamefirst
        ,SpsNameLast = ConNameLast
        ,SpsDateOfBirth = ConDateOfBirth
        ,SpsRelationship = CASE
                                WHEN ConRelationship = 'SPS' THEN 'Spouse'
                                WHEN ConRelationship = 'DP' THEN 'Partner'
                            END
        ,SpsGender = ConGender
    INTO dbo.U_ESTDLIFNEI_Spouse
    FROM dbo.Contacts
    JOIN dbo.U_dsi_BDM_DepDeductions WITH (NOLOCK)
        ON DbnDepRecID = ConSystemID
        AND DbnFormatCode = @FormatCode
        AND DbnValidForExport = 'Y'
    WHERE ConRelationship IN ('SPS','DP')
    ;
    --==========================================
    -- Build Driver Tables
    --==========================================
    ---------------------------------
    -- DETAIL RECORD - U_ESTDLIFNEI_drvTbl
    ---------------------------------
    IF OBJECT_ID('U_ESTDLIFNEI_drvTbl','U') IS NOT NULL
        DROP TABLE dbo.U_ESTDLIFNEI_drvTbl;
    SELECT DISTINCT
         drvEEID = xEEID
        ,drvCoID = xCoID
        ,drvDepRecID = CONVERT(varchar(12),'1') --DELETE IF NOT USING DEPENDENT DATA
        ,drvInitialSort = EepSSN
        -- standard fields above and additional driver fields below
        ,drvSSN = eepSSN
        ,drvNameFirst = EepNameFirst
        ,drvNameLast = EepNameLast
        ,drvDateOfBirth = EepDateOfBirth
        ,drvGender = EepGender
        ,drvWorkState = EepAddressState
        ,drvDateOfLastHire = EecDateOfLastHire
        ,drvMemberEligibilityDate = EecDateOfLastHire --dbo.dsi_fnGetMinMaxDates('MAX', EecDateOfLastHire, ISNULL((SELECT MAX(AudDateTime) FROM dbo.U_ESTDLIFNEI_Audit WHERE audEEID = xEEID),''))
        ,drvMemberEffectiveDate = '' --(SELECT MAX(EedBenStartDate) FROM U_dsi_BDM_EmpDeductions WHERE EedEEID = xEEID AND EedCoID = xCOID AND EedFormatCode = @FormatCode AND EedValidForExport = 'Y')
        ,drvDateOfTermination = CASE WHEN EecEmplStatus = 'T' THEN EecDateOfTermination END
        ,drvEarningsAmount = EecAnnSalary
        ,drvEarningsMode = 'A'
        ,drvEarningsAmountEffDate = ISNULL((SELECT MAX(EjhJobEffDate) FROM dbo.EmpHJob WITH (NOLOCK) WHERE EjhEEID = xEEID AND EjhIsRateChange = 'Y'),EecDateOfLastHire)
        ,drvQualifyingEventDate = ''
        ,drvSpousefirstName = SpsNameFirst
        ,drvSpouseLastName = SpsNameLast
        ,drvSpouseDateOfBirth = SpsDateOfBirth
        ,drvRelationshipToMember = SpsRelationship
        ,drvSpouseGender = SpsGender
        ,drvBLProductID = CASE WHEN BL.EedEEID IS NOT NULL THEN 'BL' END
        ,drvBLEffDate = BL.EedBenStartDate
        ,drvBLTermDate = BL.EedBenStopDate
        ,drvBAProductID = CASE WHEN BA.EedEEID IS NOT NULL THEN 'BA' END
        ,drvBAEffDate = BA.EedBenStartDate
        ,drvBATermDate = BA.EedBenStopDate
        ,drvALProductID = CASE WHEN AL.EedEEID IS NOT NULL THEN 'AL' END
        ,drvALEffDate = AL.EedBenStartDate
        ,drvALTermDate = AL.EedBenStopDate
        ,drvALEffAmt = ISNULL(CONVERT(VARCHAR,AL.EedBenAmt),'')
        ,drvAAProductID = CASE WHEN AA.EedEEID IS NOT NULL THEN 'AA' END
        ,drvAAEffDate = AA.EedBenStartDate
        ,drvAATermDate = AA.EedBenStopDate
        ,drvAAEffAmt = ISNULL(CONVERT(VARCHAR,AA.EedBenAmt),'')
        ,drvASLProductID = CASE WHEN ASL.EedEEID IS NOT NULL THEN 'ASL' END
        ,drvASLEffDate = ASL.EedBenStartDate
        ,drvASLTermDate = ASL.EedBenStopDate
        ,drvASLEffAmt = ISNULL(CONVERT(VARCHAR,ASL.EedBenAmt),'')
        ,drvASAProductID = CASE WHEN ASA.EedEEID IS NOT NULL THEN 'ASA' END
        ,drvASAEffDate = ASA.EedBenStartDate
        ,drvASATermDate = ASA.EedBenStopDate
        ,drvASAEffAmt = ISNULL(CONVERT(VARCHAR,ASA.EedBenAmt),'')
        ,drvACLProductID = CASE WHEN ACL.EedEEID IS NOT NULL THEN 'ACL' END
        ,drvACLEffDate = ACL.EedBenStartDate
        ,drvACLTermDate = ACL.EedBenStopDate
        ,drvACLEffAmt = ISNULL(CONVERT(VARCHAR,ACL.EedBenAmt),'')
        ,drvACAProductID = CASE WHEN ACA.EedEEID IS NOT NULL THEN 'ACA' END
        ,drvACAEffDate = ACA.EedBenStartDate
        ,drvACATermDate = ACA.EedBenStopDate
        ,drvACAEffAmt = ISNULL(CONVERT(VARCHAR,ACA.EedBenAmt),'')
        ,drvXDLProductID = CASE WHEN XDL.EedEEID IS NOT NULL THEN 'XDL' END
        ,drvXDLEffDate = XDL.EedBenStartDate
        ,drvXDLTermDate = XDL.EedBenStopDate
        ,drvXDLElectionIndicator = CASE WHEN XDL.EedEEID IS NOT NULL THEN 'Family' END
        ,drvXDLEffAmt = ISNULL(CONVERT(VARCHAR,XDL.EedBenAmt),'')
        ,drvXDAProductID = CASE WHEN XDA.EedEEID IS NOT NULL THEN 'XDA' END
        ,drvXDAEffDate = XDA.EedBenStartDate
        ,drvXDATermDate = XDA.EedBenStopDate
        ,drvXDAElectionIndicator = CASE WHEN XDA.EedEEID IS NOT NULL THEN 'Family' END
        ,drvXDAEffAmt = ISNULL(CONVERT(VARCHAR,XDA.EedBenAmt),'')
        ,drvLTProductID = CASE WHEN LT.EedEEID IS NOT NULL THEN 'LT' END
        ,drvLTEffDate = LT.EedBenStartDate
        ,drvLTTermDate = BL.EedBenStopDate
    INTO dbo.U_ESTDLIFNEI_drvTbl
    FROM dbo.U_ESTDLIFNEI_EEList WITH (NOLOCK)
    JOIN dbo.EmpComp WITH (NOLOCK)
        ON EecEEID = xEEID 
        AND EecCoID = xCoID
    JOIN dbo.EmpPers WITH (NOLOCK)
        ON EepEEID = xEEID
    JOIN dbo.U_dsi_BDM_EmpDeductions WITH (NOLOCK)
        ON EedEEID = xEEID 
        AND EedCoID = xCoID
        AND EedFormatCode = @FormatCode 
        AND EedValidForExport = 'Y'
    LEFT JOIN dbo.U_ESTDLIFNEI_Spouse WITH (NOLOCK)
        ON SpsEEID = xEEID
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions BL WITH (NOLOCK)
        ON BL.EedEEID = xEEID 
        AND BL.EedCoID = xCoID
        AND BL.EedFormatCode = @FormatCode 
        AND BL.EedValidForExport = 'Y'
        AND BL.EedDedCode IN ('GLIFE')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions BA WITH (NOLOCK)
        ON BA.EedEEID = xEEID 
        AND BA.EedCoID = xCoID
        AND BA.EedFormatCode = @FormatCode 
        AND BA.EedValidForExport = 'Y'
        AND BA.EedDedCode IN ('ADD')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions AL WITH (NOLOCK)
        ON AL.EedEEID = xEEID 
        AND AL.EedCoID = xCoID
        AND AL.EedFormatCode = @FormatCode 
        AND AL.EedValidForExport = 'Y'
        AND AL.EedDedCode IN ('LIFEE')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions AA WITH (NOLOCK)
        ON AA.EedEEID = xEEID 
        AND AA.EedCoID = xCoID
        AND AA.EedFormatCode = @FormatCode 
        AND AA.EedValidForExport = 'Y'
        AND AA.EedDedCode IN ('VADD')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions ASL WITH (NOLOCK)
        ON ASL.EedEEID = xEEID 
        AND ASL.EedCoID = xCoID
        AND ASL.EedFormatCode = @FormatCode 
        AND ASL.EedValidForExport = 'Y'
        AND ASL.EedDedCode IN ('LIFES')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions ASA WITH (NOLOCK)
        ON ASA.EedEEID = xEEID 
        AND ASA.EedCoID = xCoID
        AND ASA.EedFormatCode = @FormatCode 
        AND ASA.EedValidForExport = 'Y'
        AND ASA.EedDedCode IN ('VADDS')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions ACL WITH (NOLOCK)
        ON ACL.EedEEID = xEEID 
        AND ACL.EedCoID = xCoID
        AND ACL.EedFormatCode = @FormatCode 
        AND ACL.EedValidForExport = 'Y'
        AND ACL.EedDedCode IN ('LIFEC')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions ACA WITH (NOLOCK)
        ON ACA.EedEEID = xEEID 
        AND ACA.EedCoID = xCoID
        AND ACA.EedFormatCode = @FormatCode 
        AND ACA.EedValidForExport = 'Y'
        AND ACA.EedDedCode IN ('VADDC')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions XDL WITH (NOLOCK)
        ON XDL.EedEEID = xEEID 
        AND XDL.EedCoID = xCoID
        AND XDL.EedFormatCode = @FormatCode 
        AND XDL.EedValidForExport = 'Y'
        AND XDL.EedDedCode IN ('LIFEF')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions XDA WITH (NOLOCK)
        ON XDA.EedEEID = xEEID 
        AND XDA.EedCoID = xCoID
        AND XDA.EedFormatCode = @FormatCode 
        AND XDA.EedValidForExport = 'Y'
        AND XDA.EedDedCode IN ('LIFEF')
    LEFT JOIN dbo.U_dsi_BDM_EmpDeductions LT WITH (NOLOCK)
        ON LT.EedEEID = xEEID 
        AND LT.EedCoID = xCoID
        AND LT.EedFormatCode = @FormatCode 
        AND LT.EedValidForExport = 'Y'
        AND LT.EedDedCode IN ('LTD')
    ;
    ---------------------------------
    -- TRAILER RECORD
    ---------------------------------
    IF OBJECT_ID('U_ESTDLIFNEI_Trailer','U') IS NOT NULL
        DROP TABLE dbo.U_ESTDLIFNEI_Trailer;
    SELECT DISTINCT
         drvRecordCount = (SELECT COUNT(*) FROM dbo.U_ESTDLIFNEI_drvTbl WITH (NOLOCK))
    INTO dbo.U_ESTDLIFNEI_Trailer
    ;

    --==========================================
    -- Set FileName
    --==========================================
    IF (dbo.dsi_fnVariable(@FormatCode,'UseFileName') = 'N')
    BEGIN
        UPDATE dbo.U_dsi_Parameters
            SET ExportFile = CASE WHEN dbo.dsi_fnVariable(@FormatCode,'Testing') = 'Y' THEN 'Test_ESTDLIFNEI_' + CONVERT(VARCHAR(8),GETDATE(),112) + '.txt'
                                  WHEN @ExportCode LIKE 'OE%' THEN 'OE_Filename_' + CONVERT(VARCHAR(8),GETDATE(),112) + '.txt'
                                  ELSE 'ESTDLIFNEI_' + CONVERT(VARCHAR(8),GETDATE(),112) + '.txt'
                             END
        WHERE FormatCode = @FormatCode;
    END

END;
/**********************************************************************************

--Alter the View
ALTER VIEW dbo.dsi_vwESTDLIFNEI_Export AS
    SELECT TOP 20000000 Data
    FROM dbo.U_ESTDLIFNEI_File (NOLOCK)
    ORDER BY RIGHT(RecordSet,2), InitialSort, SubSort;

--Check out AscDefF
SELECT * FROM dbo.AscDefF
WHERE AdfHeaderSystemID LIKE 'ESTDLIFNEI%'
ORDER BY AdfSetNumber, AdfFieldNumber;

--Update Dates
UPDATE dbo.AscExp
    SET expLastStartPerControl = '201803071'
       ,expStartPerControl     = '201803071'
       ,expLastEndPerControl   = '201803149'
       ,expEndPerControl       = '201803149'
WHERE expFormatCode = 'ESTDLIFNEI';

**********************************************************************************/
GO
CREATE VIEW dbo.dsi_vwESTDLIFNEI_Export AS 
    SELECT TOP 200000000 Data FROM dbo.U_ESTDLIFNEI_File WITH (NOLOCK)
    ORDER BY RIGHT(RecordSet,2), InitialSort